{
  "name": "azure-arc-jumpstart-app",
  "version": "0.1.0",
  "homepage": "https://arpitjindal-pythond.github.io/azure-arc-jumpstartapp",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy" : "git submodule update --init --recursive && git submodule foreach --recursive git fetch && git submodule foreach --recursive git checkout origin/main && rsync -uav ../arc_jumpstart_docs/docs/** ./public/docs/ && rsync -uav ../arc_jumpstart_docs/img/** ./public/img/ && npm cache clean --force && rm -rf node_modules && rm -rf package-lock.json && npm install && npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "set \"CI=false\" && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.0.0"
  }
}
